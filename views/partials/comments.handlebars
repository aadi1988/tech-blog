<!-- comment partials-->
<div class="comments">
  {{#each this}}
  <section class="comment">
    <ol>
      <li>
          <div class="meta">
              {{user.username}} on {{format_date created_at}}
          </div>
          <div class="text">
              {{comment_text}}
          </div>
      </li>
      
    </ol>
   
  </section>
  {{/each}}
</div>